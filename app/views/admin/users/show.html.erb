<div class="right-inner">
<center>
<h1 class="heading"> USER DETAILS</h1>
</center>
  <div class="form-show-body width600">
      <form>
        <div class="form-group">
          <div class="row">
            <label class="col-sm-2"> Name </label>
            <div class="col-sm-8">
               <%= @user.try(:name) %>
            </div>
          </div>
        </div>
       
        <div class="form-group">
          <div class="row">
            <label class="col-sm-2"> Email  </label>
            <div class="col-sm-8">
              <%= @user.try(:email) %>
            </div>
          </div>
        </div>
       
        <div class="form-group">
          <div class="row">
            <label class="col-sm-2">Mobile Number  </label>
            <div class="col-sm-8">
              <%= @user.try(:mobile) %>
          </div>
        </div>
        </div>
        
        <div class="form-group">
          <div class="row">
            <label class="col-sm-2">Date of Birth </label>
            <div class="col-sm-8">
                <%= @user.try(:dob) %>
            </div>
          </div>
        </div>

        <div class="form-group">
          <div class="row">
            <label class="col-sm-2">Gender </label>
            <div class="col-sm-8">
                <%= @user.try(:gender).eql?("0") ? "Male" : "Female" %>
            </div>
          </div>
        </div>


        
        <div class="update-btn text-center">
          <%= link_to 'BACK', admin_users_path, class: "btn btn-red btn-common" %>   
        </div>
      </form>
    </div>

</div>  


  <link rel="stylesheet" href="//code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css">
  <link rel="stylesheet" href="/resources/demos/style.css">
  <script src="https://code.jquery.com/jquery-1.12.4.js"></script>
  <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>
  <script>
  $( function() {
    $( "#slider-range" ).slider({
      range: true,
      min: 0,
      max: 500,
      values: [ '<%= @user&.min_behaviour %>', '<%= @user&.max_behaviour %>' ],
      slide: function( event, ui ) {
        $( "#amount" ).val( "$" + ui.values[ 0 ] + " - $" + ui.values[ 1 ] );
      }
    });
    $( "#amount" ).val( "$" + $( "#slider-range" ).slider( "values", 0 ) +
      " - $" + $( "#slider-range" ).slider( "values", 1 ) );
  } );

  $('#slider-range').slider({ disabled: true });
  </script>