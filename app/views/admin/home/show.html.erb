<!-- Content Wrapper. Contains page content -->
<div class="content-wrapper">
  <!-- Content Header (Page header) -->
  <section class="content-header">
    <h1> Admin User Details</h1>

  </section>

  <!-- Main content -->
  <section class="content">

   <div class="box box-info">
     <div class="text-right attendee-cent-s form-group">
      <%= link_to "Edit", edit_admin_home_path, :class => "btn btn-default btn-green" %>
     </div>
          <!-- /.box-header -->
      <div class="box-body box-profile">
          <div class="profile-box">
          <div class="form-group has-feedback text-center">
            <%= @admin_user.image.present? ? image_tag(@admin_user.image.try(:url),:width => 100, :height => 100, :class => 'profile-ab') : image_tag("/1.png",:width => 100, :height => 100, :class => 'profile-ab') %>
          </div>
          <h3 class="profile-username text-center"><%= @admin_user.name%></h3>
          <p class="text-muted text-center"><%#= @admin_user.address%></p>

        <div class="profile-inner">
          <div class="p-blocks">
              <label class="view-a left-a">Name</label>
              <span class="view-a right-a"><%= @admin_user.name%></span>
          </div>
          <div class="p-blocks">
              <label class="view-a left-a">Email</label>
              <span class="view-a right-a"><%= @admin_user.email%></span>
          </div>
          <!-- <div class="p-blocks">
              <label class="view-a left-a">Address</label>
              <span class="view-a right-a"><%#= @admin_user.address%></span>
          </div> -->
      </div>

          </div>
      </div>
    </div>

  </section>
  <!-- /.content -->
</div>
<!-- /.content-wrapper -->
