<div class="right-inner">

<h1 class="heading">FEEDBACK  DETAIL</h1>
  <div class="form-show-body">
      <form>
        <div class="form-group">
          <div class="row">
            <label class="col-sm-2"> Username </label>
            <div class="col-sm-8">
              <%= @feedback.user.name  %>
            </div>
          </div>
        </div>
       
        <div class="form-group">
          <div class="row">
            <label class="col-sm-2">  Description  </label>
            <div class="col-sm-8">
              <%=  @feedback.description %>
            </div>
          </div>
        </div>
       
        <div class="form-group">
          <div class="row">
            <label class="col-sm-2"> Date </label>
            <div class="col-sm-8">
              <%= @feedback.created_at.strftime("%d %B, %Y %I:%M %p") %>
          </div>
        </div>
        
        <div class="update-btn text-center">
          <%= link_to 'BACK', admin_feedbacks_path, class: "btn btn-danger btn-common" %>
        </div>
      </form>
    </div>
</div>  